@{
    ViewData["Title"] = "Portfolio-Index";
}

<!-- Page Title
================================================== -->
<div id="page-title">
    <div class="row">
        <div class="ten columns centered text-center">
            <h1>Our Amazing Works<span>.</span></h1>

            <p></p>
        </div>
    </div>
</div>
<!-- Page Title End-->
<!-- Content
================================================== -->
<div class="content-outer">
